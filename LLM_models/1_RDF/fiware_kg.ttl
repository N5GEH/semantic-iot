@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix rec: <https://w3id.org/rec#> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Hotel Entity
ex:Hotel:hotel:aachen:001 rdf:type brick:Location ;
    rdfs:label "hotel:aachen:001" .

# Hotel Rooms
ex:HotelRoom:room_base_1 rdf:type rec:Bedroom ;
    rdfs:label "room_base_1" ;
    brick:hasLocation ex:Hotel:hotel:aachen:001 .

ex:HotelRoom:room_base_2 rdf:type rec:Bedroom ;
    rdfs:label "room_base_2" ;
    brick:hasLocation ex:Hotel:hotel:aachen:001 .

# Ambient Temperature Sensor
ex:AmbientTemperatureSensor rdf:type brick:Outside_Air_Temperature_Sensor ;
    brick:hasLocation ex:Hotel:hotel:aachen:001 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/AmbientTemperatureSensor/attrs/temperatureAmb/value> .

# Room 1 Sensors and Actuators
ex:TemperatureSensor:room_base_1 rdf:type brick:Temperature_Sensor ;
    brick:hasLocation ex:HotelRoom:room_base_1 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/TemperatureSensor:room_base_1/attrs/temperature/value> .

ex:CO2Sensor:room_base_1 rdf:type brick:CO2_Sensor ;
    brick:hasLocation ex:HotelRoom:room_base_1 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/CO2Sensor:room_base_1/attrs/co2/value> .

ex:PresenceSensor:room_base_1 rdf:type brick:Occupancy_Sensor ;
    brick:hasLocation ex:HotelRoom:room_base_1 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/PresenceSensor:room_base_1/attrs/pir/value> .

ex:FreshAirVentilation:room_base_1 rdf:type brick:Ventilation_Air_System ;
    brick:hasLocation ex:HotelRoom:room_base_1 ;
    brick:hasPoint ex:FreshAirVentilation:room_base_1:airFlowSetpoint .

ex:FreshAirVentilation:room_base_1:airFlowSetpoint rdf:type brick:Air_Flow_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/FreshAirVentilation:room_base_1/attrs/airFlowSetpoint/value> .

ex:RadiatorThermostat:room_base_1 rdf:type brick:Radiator ;
    brick:hasLocation ex:HotelRoom:room_base_1 ;
    brick:hasPoint ex:RadiatorThermostat:room_base_1:temperatureSetpoint .

ex:RadiatorThermostat:room_base_1:temperatureSetpoint rdf:type brick:Temperature_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/RadiatorThermostat:room_base_1/attrs/temperatureSetpoint/value> .

ex:CoolingCoil:room_base_1 rdf:type brick:Cooling_Coil ;
    brick:hasLocation ex:HotelRoom:room_base_1 ;
    brick:hasPoint ex:CoolingCoil:room_base_1:fanSpeed .

ex:CoolingCoil:room_base_1:fanSpeed rdf:type brick:Fan_Speed_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/CoolingCoil:room_base_1/attrs/fanSpeed/value> .

# Room 2 Sensors and Actuators
ex:TemperatureSensor:room_base_2 rdf:type brick:Temperature_Sensor ;
    brick:hasLocation ex:HotelRoom:room_base_2 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/TemperatureSensor:room_base_2/attrs/temperature/value> .

ex:CO2Sensor:room_base_2 rdf:type brick:CO2_Sensor ;
    brick:hasLocation ex:HotelRoom:room_base_2 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/CO2Sensor:room_base_2/attrs/co2/value> .

ex:PresenceSensor:room_base_2 rdf:type brick:Occupancy_Sensor ;
    brick:hasLocation ex:HotelRoom:room_base_2 ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/PresenceSensor:room_base_2/attrs/pir/value> .

ex:FreshAirVentilation:room_base_2 rdf:type brick:Ventilation_Air_System ;
    brick:hasLocation ex:HotelRoom:room_base_2 ;
    brick:hasPoint ex:FreshAirVentilation:room_base_2:airFlowSetpoint .

ex:FreshAirVentilation:room_base_2:airFlowSetpoint rdf:type brick:Air_Flow_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/FreshAirVentilation:room_base_2/attrs/airFlowSetpoint/value> .

ex:RadiatorThermostat:room_base_2 rdf:type brick:Radiator ;
    brick:hasLocation ex:HotelRoom:room_base_2 ;
    brick:hasPoint ex:RadiatorThermostat:room_base_2:temperatureSetpoint .

ex:RadiatorThermostat:room_base_2:temperatureSetpoint rdf:type brick:Temperature_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/RadiatorThermostat:room_base_2/attrs/temperatureSetpoint/value> .

ex:CoolingCoil:room_base_2 rdf:type brick:Cooling_Coil ;
    brick:hasLocation ex:HotelRoom:room_base_2 ;
    brick:hasPoint ex:CoolingCoil:room_base_2:fanSpeed .

ex:CoolingCoil:room_base_2:fanSpeed rdf:type brick:Fan_Speed_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/CoolingCoil:room_base_2/attrs/fanSpeed/value> .