@prefix bacnet: <http://data.ashrae.org/bacnet/2020#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix bsh: <https://brickschema.org/schema/BrickShape#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix qudtqk: <http://qudt.org/vocab/quantitykind/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rec: <https://w3id.org/rec#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix sdo: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tag: <https://brickschema.org/schema/BrickTag#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Hotel Entity
<http://example.com/Hotel/example_hotel> a rec:Shelter ;
    sdo:name "example_hotel" .

# Ambient Temperature Sensor
<http://example.com/AmbientTemperatureSensor> a brick:Temperature_Sensor ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/AmbientTemperatureSensor/attrs/temperatureAmb/value> ;
    brick:hasLocation <http://example.com/Hotel/example_hotel> .

# Hotel Room
<http://example.com/HotelRoom/example_room> a rec:Room ;
    sdo:name "example_room" ;
    brick:hasLocation <http://example.com/Hotel/example_hotel> .

# Temperature Sensor
<http://example.com/TemperatureSensor/example_room> a brick:Temperature_Sensor ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/TemperatureSensor:example_room/attrs/temperature/value> ;
    brick:hasLocation <http://example.com/HotelRoom/example_room> .

# CO2 Sensor
<http://example.com/CO2Sensor/example_room> a brick:CO2_Sensor ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/CO2Sensor:example_room/attrs/co2/value> ;
    brick:hasLocation <http://example.com/HotelRoom/example_room> .

# Presence Sensor
<http://example.com/PresenceSensor/example_room> a brick:Occupancy_Sensor ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/PresenceSensor:example_room/attrs/pir/value> ;
    brick:hasLocation <http://example.com/HotelRoom/example_room> .

# Fresh Air Ventilation
<http://example.com/FreshAirVentilation/example_room> a brick:Ventilation_Air_System ;
    brick:hasLocation <http://example.com/HotelRoom/example_room> .

# Air Flow Setpoint (Extra Node)
<http://example.com/airFlowSetpoint/FreshAirVentilation/example_room> a brick:Air_Flow_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/FreshAirVentilation:example_room/attrs/airFlowSetpoint/value> ;
    brick:isPointOf <http://example.com/FreshAirVentilation/example_room> .

# Radiator Thermostat
<http://example.com/RadiatorThermostat/example_room> a brick:Radiator ;
    brick:hasLocation <http://example.com/HotelRoom/example_room> .

# Temperature Setpoint (Extra Node)
<http://example.com/temperatureSetpoint/RadiatorThermostat/example_room> a brick:Temperature_Setpoint ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/RadiatorThermostat:example_room/attrs/temperatureSetpoint/value> ;
    brick:isPointOf <http://example.com/RadiatorThermostat/example_room> .

# Cooling Coil
<http://example.com/CoolingCoil/example_room> a brick:Cooling_Coil ;
    brick:hasLocation <http://example.com/HotelRoom/example_room> .

# Fan Speed (Extra Node)
<http://example.com/fanSpeed/CoolingCoil/example_room> a brick:Fan_Speed_Command ;
    rdf:value <https://fiware.eonerc.rwth-aachen.de/v2/entities/CoolingCoil:example_room/attrs/fanSpeed/value> ;
    brick:isPointOf <http://example.com/CoolingCoil/example_room> .