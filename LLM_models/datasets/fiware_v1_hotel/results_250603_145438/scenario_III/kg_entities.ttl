@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix rec: <https://w3id.org/rec#> .

<http://example.com/AmbientTemperatureSensor/AmbientTemperatureSensor> a brick:Temperature_Sensor ;
    brick:hasLocation <http://example.com/Hotel/Hotel:example_hotel> .

<http://example.com/CO2Sensor/CO2Sensor:example_room> a brick:CO2_Sensor ;
    brick:hasLocation <http://example.com/HotelRoom/HotelRoom:example_room> .

<http://example.com/PresenceSensor/PresenceSensor:example_room> a brick:Occupancy_Sensor ;
    brick:hasLocation <http://example.com/HotelRoom/HotelRoom:example_room> .

<http://example.com/TemperatureSensor/TemperatureSensor:example_room> a brick:Temperature_Sensor ;
    brick:hasLocation <http://example.com/HotelRoom/HotelRoom:example_room> .

<http://example.com/CoolingCoil/CoolingCoil:example_room> a brick:Cooling_Coil ;
    brick:hasLocation <http://example.com/HotelRoom/HotelRoom:example_room> ;
    rec:relatedTo <http://example.com/fanSpeed_CoolingCoil/fanSpeed_CoolingCoil:example_room> .

<http://example.com/FreshAirVentilation/FreshAirVentilation:example_room> a brick:Ventilation_Air_System ;
    brick:hasLocation <http://example.com/HotelRoom/HotelRoom:example_room> ;
    rec:relatedTo <http://example.com/airFlowSetpoint_FreshAirVentilation/airFlowSetpoint_FreshAirVentilation:example_room> .

<http://example.com/Hotel/Hotel:example_hotel> a rec:Shelter .

<http://example.com/RadiatorThermostat/RadiatorThermostat:example_room> a brick:Radiator ;
    brick:hasLocation <http://example.com/HotelRoom/HotelRoom:example_room> ;
    rec:relatedTo <http://example.com/temperatureSetpoint_RadiatorThermostat/temperatureSetpoint_RadiatorThermostat:example_room> .

<http://example.com/airFlowSetpoint_FreshAirVentilation/airFlowSetpoint_FreshAirVentilation:example_room> a brick:Air_Flow_Setpoint ;
    rec:relatedTo <http://example.com/FreshAirVentilation/FreshAirVentilation:example_room> .

<http://example.com/fanSpeed_CoolingCoil/fanSpeed_CoolingCoil:example_room> a brick:Fan_Speed_Command ;
    rec:relatedTo <http://example.com/CoolingCoil/CoolingCoil:example_room> .

<http://example.com/temperatureSetpoint_RadiatorThermostat/temperatureSetpoint_RadiatorThermostat:example_room> a brick:Temperature_Setpoint ;
    rec:relatedTo <http://example.com/RadiatorThermostat/RadiatorThermostat:example_room> .

<http://example.com/HotelRoom/HotelRoom:example_room> a rec:Room .

