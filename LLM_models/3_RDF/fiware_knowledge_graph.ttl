@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fiware: <https://fiware.eonerc.rwth-aachen.de/v2/entities/> .

# Hotel
fiware:Hotel:example_hotel rdf:type brick:Shelter ;
    rdfs:label "example_hotel" .

# Ambient Temperature Sensor
fiware:AmbientTemperatureSensor rdf:type brick:Outside_Air_Temperature_Sensor ;
    brick:hasAmbientTemperature "0.0"^^xsd:float ;
    brick:hasLocation fiware:Hotel:example_hotel .

# Hotel Room
fiware:HotelRoom:example_room rdf:type brick:Bedroom ;
    rdfs:label "example_room" ;
    brick:hasLocation fiware:Hotel:fiware .

# Temperature Sensor in Room
fiware:TemperatureSensor:example_room rdf:type brick:Temperature_Sensor ;
    brick:hasLocation fiware:HotelRoom:example_room ;
    brick:hasAmbientTemperature "0.0"^^xsd:float .

# CO2 Sensor in Room
fiware:CO2Sensor:example_room rdf:type brick:CO2_Sensor ;
    brick:hasOutputSubstance "0.0"^^xsd:float ;
    brick:hasLocation fiware:HotelRoom:example_room .

# Presence Sensor in Room
fiware:PresenceSensor:example_room rdf:type brick:Occupancy_Sensor ;
    brick:hasLocation fiware:HotelRoom:example_room ;
    brick:connectedTo "0.0"^^xsd:float .

# Fresh Air Ventilation in Room
fiware:FreshAirVentilation:example_room rdf:type brick:Fresh_Air_Fan ;
    brick:hasPoint "0.0"^^xsd:float ;
    brick:hasLocation fiware:HotelRoom:example_room .

# Radiator Thermostat in Room
fiware:RadiatorThermostat:example_room rdf:type brick:Radiator ;
    brick:hasLocation fiware:HotelRoom:example_room ;
    brick:targetPoint "0.0"^^xsd:float .

# Cooling Coil in Room
fiware:CoolingCoil:example_room rdf:type brick:Cooling_Coil ;
    brick:portSpeed "0.0"^^xsd:float ;
    brick:hasLocation fiware:HotelRoom:example_room .